              <nodes>
                <node text="+">
                  <nodes>
                    <node text="сопровождается перераспределением жидкости из внутриклеточного во внеклеточный сектор" />
                    <node text="дегидратации клеток" />
                    <node text="гіпокалімія" />
                    <node text="можливо, гіпергідратація" />
                    <node text="гіперволемія">
