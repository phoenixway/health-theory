                        <node text="як наслідок - серцева недостатність" />
                      </nodes>
                    </node>
                    <node text="снижению осмотического давления" />
                    <node text="меньшению объема интерстициального пространства" />
                    <node text="гиперосмолярностью плазмы" />
                  </nodes>
                </node>
                <node text="-">
                  <nodes>
                    <node text="сопровождается гипергидратацией клеток" />
                    <node text="уменьшением объема циркулирующей крови." />
                  </nodes>
                </node>
